{
  "name": "forceify",
  "version": "0.2.13",
  "description": "Simple, yet powerful touch-force handler",
  "main": "forceify.umd.js",
  "browser": "forceify.umd.min.js",
  "cdn": "forceify.umd.min.js",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run minify",
    "lint": "npx standard --fix",
    "minify": "npx terser forceify.umd.js -c -m -o forceify.umd.min.js"
  },
  "standard": {
    "ignore": [
      "forceify.umd.min.js"
    ]
  },
  "keywords": [
    "3d",
    "touch",
    "force",
    "force-touch",
    "3d-touch",
    "ios",
    "js",
    "android",
    "unsupported",
    "simulate",
    "normalize",
    "cross-device",
    "javascript",
    "typescript",
    "lightweight",
    "fast",
    "osx",
    "iphone"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "devDependencies": {
    "standard": "^12.0.1",
    "terser": "^3.17.0"
  }
}
